resource "clumio_organizational_unit" "{{ ou_name }}" {
  {%- if clumio_provider_alias %}
  provider    = clumio.{{ clumio_provider_alias }}
  {%- endif %}
  name        = "{{ display_name }}"
  description = "{{ description }}"
  {%- if parent_name %}
  parent_id   = clumio_organizational_unit.{{ parent_name }}.id
  {%- endif %}
}
